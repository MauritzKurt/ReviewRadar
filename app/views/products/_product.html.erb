<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= product.description %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= product.company_id %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= product.url %>
  </p>

</div>
